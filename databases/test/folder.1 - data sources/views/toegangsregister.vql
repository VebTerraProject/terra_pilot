             ADD straat (any) OPT ANY
             ADD huisnummer (any) OPT ANY
             ADD bus (any) OPT ANY
             ADD postcode (any) OPT ANY
             ADD gemeente (any) OPT ANY
             ADD energietype (any) OPT ANY
             ADD gos (any) OPT ANY
